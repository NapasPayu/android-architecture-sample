{
  "extends": [
    "config:base",
    ":dependencyDashboard"
  ],
  "groupName": "all dependencies",
  "separateMajorMinor": false,
  "groupSlug": "all",
  "packageRules": [
    {
      "matchPackagePatterns": [
        "*"
      ],
      "groupName": "all dependencies",
      "groupSlug": "all",
      "matchPaths": [
        "gradle/libs.versions.toml",
        "build.gradle.kts"
      ]
    }
  ],
  "lockFileMaintenance": {
    "enabled": false
  }
}
